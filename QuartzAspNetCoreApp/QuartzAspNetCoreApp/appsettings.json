{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Quartz": {
    "quartz.scheduler.instanceName": "QuartzAspNetCoreApp",
    "quartz.scheduler.instanceId": "AUTO",
    "quartz.jobStore.driverDelegateType": "Quartz.Impl.AdoJobStore.PostgreSQLDelegate",
    "quartz.jobStore.dataSource": "myDS",
    "quartz.jobStore.clustered": "true",
    "quartz.dataSource.myDS.provider": "Npgsql",
    "quartz.dataSource.myDS.connectionString": "Host=localhost;Port=5432;Pooling=true;Database=QuartzAspNetCore;Username=postgres;Password=123456;"
  }
}
